#ifndef _CSTDIO
#define _CSTDIO 1

//Just dump the definitions in the global namespace
#include <stdio.h>

//Including a c++ header means functions are in the std namespace
namespace std {

	using ::printf;
	using ::vprintf;
	using ::putchar;
	using ::puts;

} //namespace std

#endif //_CSTDIO
