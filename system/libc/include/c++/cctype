#ifndef _CCTYPE
#define _CCTYPE 1

/* Just dump the definitions in the global namespace */
#include "ctype.h"

/* Including a c++ header means functions are in the std namespace */
namespace std {

using ::iscntrl;
using ::isdigit;
using ::isprint;
using ::isspace;

} /* namespace std */

#endif /* _CCTYPE */
