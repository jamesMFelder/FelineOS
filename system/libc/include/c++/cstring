#ifndef _CSTRING
#define _CSTRING 1

/* Just dump the definitions in the global namespace */
#include "string.h"

/* Including a c++ header means functions are in the std namespace */
namespace std {

using ::memcmp;

using ::memcpy;
using ::memmove;
using ::memset;

using ::strlen;

using ::strcpy;
using ::strlcpy;
using ::strncpy;

using ::strcat;
using ::strlcat;
using ::strncat;

using ::strcmp;
using ::strncmp;

using ::strstr;

} /* namespace std */

#endif /* _CSTRING */
