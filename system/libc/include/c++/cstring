#ifndef _CSTRING
#define _CSTRING 1

//Just dump the definitions in the global namespace
#include "string.h"

//Including a c++ header means functions are in the std namespace
namespace std {

	using ::memcmp;

	using ::memcpy;
	using ::memmove;
	using ::memset;

	using ::strlen;

	using ::strcpy;
	using ::strncpy;
	using ::strlcpy;

	using ::strcat;
	using ::strncat;
	using ::strlcat;

} //namespace std

#endif //_CSTRING
